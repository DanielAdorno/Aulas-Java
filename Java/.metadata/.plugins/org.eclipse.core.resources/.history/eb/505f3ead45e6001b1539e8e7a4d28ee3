package TesteIf;

public class TesteComIfs {

	public static void main(String[] args) {
		
		System.out.println("Testando Condicionias");
		int idade = 10;
		if (idade >= 18) {
			System.out.println("Parabéns você é maior de Idade.");
		}else {
		System.out.println("Desculpa, você ainda é menor.");
	}

}
}
